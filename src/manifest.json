{
  "name": "sensors_extention",
  "description": "A Vue.js web extension",
  "version": 1.0,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "sensors_extention",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "tab/tab.html"
  },
  "permissions": [
    "serial"
  ]
}
